{
  "expo": {
    "name": "Kairox Lab",
    "slug": "kx-friday",
    "version": "1.0.0b",
    "orientation": "portrait",
    "icon": "./assets/img/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "kairox",
    "splash": {
      "image": "./assets/img/full-logo-white.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/icon.png",
        "backgroundColor": "#333333"
      },
      "package": "com.darthkenar.kxfriday"
    },
    "web": {
      "favicon": "./assets/img/favicon.png"
    },
    "plugins": ["expo-router"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1132a21f-218a-4784-a259-f229ad1ed298"
      }
    }
  }
}
